# code with fun

from turtle import *
import colorsys
speed(0)
bgcolor("black")

def leaf():
    color("white")
    fillcolor("green")
    begin_fill()
    circle(100,90)
    lt(90)
    circle(100,90)
    end_fill()
rt(90)
color("white")
fillcolor("green")
begin_fill()
fd(300)
lt(90)
fd(10)
lt(90)
fd(300)
lt(90)
fd(10)
end_fill()

lt(90)
fd(300)
rt(135)
fd(50)
leaf()
rt(90)
pensize(3)
fd(50)
lt(45)
fd(10)
lt(60)

pensize(1)








